<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/start_remote_service"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:background="@color/background"
        android:gravity="center"
        android:text="@string/start_remote_process" />

    <TextView
        android:id="@+id/transcode_remote_process"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:background="@color/background"
        android:gravity="center"
        android:text="@string/transcode_remote_process" />

    <TextView
        android:id="@+id/transcode_io_thread"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:background="@color/background"
        android:gravity="center"
        android:text="@string/transcode_io_thread" />


</LinearLayout>